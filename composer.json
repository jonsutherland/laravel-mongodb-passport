{
    "name": "jonsutherland/laravel-mongodb-passport",
    "description": "A package to allow laravel/passport use with jenssegers/laravel-mongodb",
    "homepage": "https://github.com/mvjacobs/laravel-mongodb-passport",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-mongodb",
        "laravel-mongodb-passport",
        "laravel-passport",
        "mongodb",
        "passport",
        "mvjacobs"
    ],
    "require": {
        "php": ">=7.1",
        "jenssegers/mongodb": "3.3.* || 3.4.* || 3.5.* || 3.6.* || 3.7.* || 3.8.* || 3.9.* || dev-master", 
        "laravel/passport": "6.0.* || 7.0.* || 7.4.* || 7.5.* || ^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "mvjacobs\\Mongodb\\": "src"
        }
    },
    "authors": [
        {
            "name": "jonsutherland",
            "email": "support@mvjacobs.com",
            "role": "Support"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "mvjacobs\\Mongodb\\MongodbPassportServiceProvider"
            ]
        }
    }
}
